<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="com.nav1.nav1.controller.Detalle">
	<App>
		<pages>
			<Page title="Title" showNavButton="true" navButtonPress="onVolver">
				<content>
				    <ObjectHeader xmlns="sap.m" title="{Nombre} {Apellido}" number="100.00" numberUnit="$" id="header0">
				        <statuses>
				            <ObjectStatus text="Success" state="Success" id="status0"/>
				            
				        </statuses>
				        <attributes>
				            <ObjectAttribute text="10 units" id="attribute0"/>
				            
				        </attributes>
				        </ObjectHeader>
				    <Table xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" items="{LosPacientes}" noDataText="Drop column list items here and columns in the area above" id="table0" cd:w5g.dt.context="/datos/LosPacientes">
				        <items>
				            <ColumnListItem type="Active" id="item0">
				                <cells>
				                    <Text id="text1" text="{Campo1}"/>
				                    <Text text="{Campo2}" id="text2"/>
				                    <Text text="{Campo3}" id="text3"/></cells>
				            </ColumnListItem>
				        </items>
				        <columns>
				            <Column id="column0">
				                <header>
				                    <Label text="Header 1" id="label0"/>
				                </header>
				            </Column>
				            <Column id="column1">
				                <header>
				                    <Label text="Header 2" id="label1"/>
				                </header>
				            </Column>
				            <Column id="column2">
				                <header>
				                    <Label text="Header 3" id="label2"/>
				                </header>
				            </Column></columns></Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>