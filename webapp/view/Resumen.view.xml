<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:html="http://www.w3.org/1999/xhtml" xmlns="sap.m" controllerName="com.nav1.nav1.controller.View1" displayBlock="true">
	<Page title="{i18n>title}">
		<content>
			<Table xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" items="{/datos}" noDataText="Drop column list items here and columns in the area above" id="table0" cd:w5g.dt.context="/datos">
				<items>
					<ColumnListItem type="Navigation" id="item0" press="onPress">
						<cells>
							<Text text="{Rut}" id="text0"/>
							<Text id="text1" text="{Nombre}"/>
							<Text id="text2" text="{Apellido}"/></cells>
					</ColumnListItem>
				</items>
				<columns>
					<Column id="column0">
						<header>
							<Label text="Header 1" id="label0"/>
						</header>
					</Column>
					<Column id="column1">
						<header>
							<Label text="Header 2" id="label1"/>
						</header>
					</Column>
					<Column id="column2">
						<header>
							<Label text="Header 3" id="label2"/>
						</header>
					</Column>
				</columns>
			</Table>
		</content>
	</Page>
</mvc:View>